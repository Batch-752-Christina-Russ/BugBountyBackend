import com.revature.service.BugReportService;
import com.revature.service.UserService;

@SpringBootApplication
public class BugBountyBeApplication {

	public static void main(String[] args) {
		//SpringApplication.run(BugBountyBeApplication.class, args);
		
		UserService us = new UserService();
		BugReportService brs = new BugReportService();
		
		User derp = new User(0, "Derpvin", "pass", 100000000, new Role(2, "admin"));
		us.saveUser(derp);
		System.out.println(us.getAllUsers());
	}

}
